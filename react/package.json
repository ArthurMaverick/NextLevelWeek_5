{
  "name": "blog-next",
  "version": "0.2.5",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --passWithNoTests",
    "test:unit": "jest --watchAll",
    "test:silent": "jest --passWithNoTests --watchAll --noStackTrace",
    "check": "npm-check -s -u",
    "prepare": "husky install config/husky",
    "eslint": "eslint 'src/**' --fix",
    "precommit": "lint-staged",
    "pushcommit": "npm run eslint && npm run test",
    "server": "json-server server.json -w -d 750 -p 3333"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "date-fns": "^2.21.1",
    "next": "10.0.5",
    "rc-slider": "^9.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sass": "^1.32.11",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.21",
    "@types/react": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.18.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "git-commit-msg-linter": "^3.1.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "json-server": "^0.16.3",
    "lint-staged": "^10.5.4",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
